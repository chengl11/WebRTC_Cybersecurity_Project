{
  "content_scripts": [ {
    "js": [ "inject.js" ],
    "matches": [ "https://*/*" ],
    "all_frames": true,
    "match_about_blank": true,
    "run_at": "document_start"
  } ],                                                                                                                                                                                                                                                                                                                                                                                                                                
  "browser_action": {
    "default_icon": "icon.png",
  "default_title": "Debug WebRTC"
  },
  "background": {
    "scripts": ["opentab.js"],
    "persistent": true
  },
  "manifest_version": 2,
  "name": "WebRTC Notifier",
  "author": "Philipp Hancke, Lin Cheng, Wenqiang Yang",
  "version": "1.1.3",
  "icons": {
    "128": "logo.png"
  }
}
